# Given sorted data
data <- c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 
          33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70)

# (a) Compute Mean and Median
mean_value <- mean(data)
median_value <- median(data)

# (b) Compute Mode
get_mode <- function(x) {
  uniq_vals <- unique(x)
  freq <- tabulate(match(x, uniq_vals))
  modes <- uniq_vals[freq == max(freq)]
  return(modes)
}

mode_values <- get_mode(data)

# (c) Compute Midrange
midrange_value <- (min(data) + max(data)) / 2

# (d) Compute Quartiles
Q1 <- quantile(data, 0.25)
Q3 <- quantile(data, 0.75)

# Print results
cat("Mean:", mean_value, "\n")
cat("Median:", median_value, "\n")
cat("Mode:", mode_values, "\n")
cat("Midrange:", midrange_value, "\n")
cat("First Quartile (Q1):", Q1, "\n")
cat("Third Quartile (Q3):", Q3, "\n")
